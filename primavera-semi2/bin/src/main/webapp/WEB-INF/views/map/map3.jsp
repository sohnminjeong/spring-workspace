    <script>
      $(function () {
        var bag = $("#bag");
        var clothes = $("#clothes");
        var wallet = $("#wallet");

        var lastScroll = 0;
        $(window).scroll(function (event) {

          var st = $(this).scrollTop();

          bag.removeClass("titleAni");
          clothes.removeClass("titleAni");
          wallet.removeClass("titleAni");

          if (st > lastScroll) {
            // scroll : down
            if (st > 750) {
              bag.addClass("titleAni");
            }
            if (st >= 1750) {
              clothes.addClass("titleAni");
            }
            if (st > 2650) {
              wallet.addClass("titleAni");
            }
          }
          else {
            // scroll : up
            if (st < 850) {
              bag.addClass("titleAni");
            }
            if (st < 1750) {
              clothes.addClass("titleAni");
            }
            if (st < 2620) {
              wallet.addClass("titleAni");
            }
          }

          lastScroll = st;
        });
      });
    </script>
